<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.maxenceaici.bikeblocks" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="BluetoothSerial">
        <param name="android-package" value="com.megster.cordova.BluetoothSerial" />
    </feature>
    <name>BikeBlocks</name>
    <description>
        Connected bike app.
    </description>
    <author email="maxence.aici@edu.esiee.fr" href="http://maxenceaici.fr">
        Charlène Chabert, Alain Lim, Rémi Nicole and Maxence Aïci
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
